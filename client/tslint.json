{
  "defaultSeverity": "error",
  "extends": ["tslint:latest", "tslint-react", "tslint-config-prettier"],
  "rulesDirectory": ["tslint-plugin-prettier"],
  "jsRules": {},
  "rules": {
    "prettier": [
      true,
      {
        "singleQuote": true,
        "trailingComma": "es5"
      }
    ],
    "trailing-comma": true,
    "member-access": false,
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "interface-name": false,
    "no-console": false
  }
}
